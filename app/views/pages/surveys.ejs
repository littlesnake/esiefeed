<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

    <header>
        <% include ../partials/header %>
    </header>

    <main>
        <div class="row">
            <div class="col-sm-9">
            
                <div class="jumbotron">
                    <h3>Surveys</h1>
                    
                    <ul>
                        <% for(var i=0; i<surveys.length; i++) {%>
                            <li>
                                <a href="/feedback/feedbacks/<%= surveys[i]._id %>">
                                    <%= surveys[i].title %>
                                </a>
                            </li>
                        <% } %> 
                    </ul>
                </div>
                
            </div>
            <div class="col-sm-3">
                
                <div class="well">
                    <h4>Recent Surveys</h4>
                </div>

            </div>
        </div>
    </main>

    <footer>
        <% include ../partials/footer %>
    </footer>
    
</body>
</html>